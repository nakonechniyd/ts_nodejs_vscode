{
    "compilerOptions": {
        "declaration": false,
        "target": "ES2020",
        "module": "ES2015",
        "sourceMap": true,
        "outDir": ".dist",
        "rootDir": "src",
        "strict": true,
        "lib": ["ES2020"],
        "types": [],
        "noUnusedParameters": true,
        "noUnusedLocals": true,
        "moduleResolution": "node",
        "forceConsistentCasingInFileNames": true
    },
    "include": ["./src"]
}
